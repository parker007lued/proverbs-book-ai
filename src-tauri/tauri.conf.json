{
  "productName": "Proverbs Book AI",
  "version": "1.0.0",
  "identifier": "com.proverbsbook.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Proverbs Book AI",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico",
      "icons/icon.png"
    ],
    "resources": [],
    "copyright": "",
    "category": "Productivity",
    "shortDescription": "AI-powered book writing application for Proverbs chapters",
    "longDescription": "Create original content, generate graphics, and assemble books about Proverbs chapters using open-source AI models"
  }
}
